# Add inputs and outputs from these tool invocations to the build variables 
EXECUTABLES += \
WS2812B.elf \

SIZE_OUTPUT += \
default.size.stdout \

OBJDUMP_LIST += \
WS2812B.list \


# All Target
all:
	+@make --no-print-directory main-build && make --no-print-directory post-build

# Main-build Target
main-build: WS2812B.elf secondary-outputs

# Tool invocations
WS2812B.elf: $(OBJS) $(USER_OBJS) ../STM32F429ZI_FLASH.ld
	arm-none-eabi-gcc -o "WS2812B.elf" @"objects.list" $(USER_OBJS) $(LIBS) -mcpu=cortex-m4 -T"../STM32F429ZI_FLASH.ld" --specs=nosys.specs -Wl,-Map="WS2812B.map" -Wl,--gc-sections -static --specs=nano.specs -mfpu=fpv4-sp-d16 -mfloat-abi=hard -mthumb -Wl,--start-group -lc -lm -Wl,--end-group
	@echo 'Finished building target: $@'
	@echo ' '

default.size.stdout: $(EXECUTABLES)
	arm-none-eabi-size  $(EXECUTABLES)
	@echo 'Finished building: $@'
	@echo ' '

WS2812B.list: $(EXECUTABLES)
	arm-none-eabi-objdump -h -S $(EXECUTABLES) > "WS2812B.list"
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) *
	-@echo ' '

post-build:
	arm-none-eabi-objcopy.exe -O ihex "WS2812B.elf" "WS2812B.hex"
	-@echo ' '

secondary-outputs: $(SIZE_OUTPUT) $(OBJDUMP_LIST)

.PHONY: all clean dependents
.SECONDARY: post-build

